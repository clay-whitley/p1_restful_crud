<div class="container">
  <form action=<%= "/notes/#{@note.id}" %> method="post">
    <input type="text" name="note[title]" value=<%= '"' + @note.title + '"' %>>
    <textarea name="note[content]"><%= @note.content %></textarea>
    <input type="hidden" name="_method" value="put" />
    <input type="submit">
  </form>
</div>